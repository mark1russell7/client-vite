{
  "$schema": "https://json.schemastore.org/package",
  "name": "@mark1russell7/client-vite",
  "version": "0.0.1",
  "description": "Vite dev server management procedures",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo",
    "postinstall": "client announce"
  },
  "sideEffects": false,
  "dependencies": {
    "zod": "^3.24.0",
    "@mark1russell7/client": "github:mark1russell7/client#main",
    "@mark1russell7/client-shell": "github:mark1russell7/client-shell#main"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/node": "^22.0.0",
    "@mark1russell7/cue": "github:mark1russell7/cue#main"
  },
  "client": {
    "procedures": "./dist/register.js"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@mark1russell7/cue",
      "@mark1russell7/client",
      "@mark1russell7/client-shell"
    ]
  }
}
